#:schema node_modules/wrangler/config-schema.json
name = "workers-toram-news"
main = "src/index.js"
compatibility_date = "2024-06-10"
compatibility_flags = ["nodejs_compat"]

workers_dev = true

[triggers]
crons = ["* * * * *"]

[[d1_databases]]
binding = "TORAM"
database_name = "toram"
database_id = "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
